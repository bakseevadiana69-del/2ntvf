class Calculator:
    def add(self, a: int, b: int) -> int:
        """Метод для сложения двух чисел."""
        return a + b

    def multiply(self, a: int, b: int) -> float:
        """Метод для умножения двух чисел, возвращающий результат в виде float."""
        return float(a * b)


class User:
    def __init__(self, username: str, email: str, age: int):
        self.username = username  # Имя пользователя
        self.email = email        # Электронная почта
        self.age = age            # Возраст пользователя
        self.__password = None    # Приватное свойство для хранения пароля

    def set_password(self, password: str):
        """Метод для установки пароля."""
        self.__password = password

    def get_user_info(self) -> str:
        """Метод для получения информации о пользователе."""
        return f"Username: {self.username}, Email: {self.email}, Age: {self.age}"


# Пример использования
if __name__ == "__main__":
    # Создание экземпляра класса Calculator
    calculator = Calculator()
    print("Сложение:", calculator.add(5, 10))
    print("Умножение:", calculator.multiply(5, 10))

    # Создание экземпляра класса User
    user = User("john_doe", "john@example.com", 30)
    user.set_password("securepassword123")
    print(user.get_user_info())
